#ifndef HARL_HPP
#define HARL_HPP

#pragma once
#include <string>
#include <cstdlib>
#include <ctime>
#include <iostream>

class Harl {
	private:
	void	debug(void);
	void	info(void);
	void	warning(void);
	void	error(void);

	public:
	void	complain(std::string level);
};
#endif
