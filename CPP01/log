#ifndef HARL_HPP
#define HARL_HPP

#pragma once

#include <iostream>
#include <string>
#include <cstdlib>
#include <ctime>

class Harl {
	private:
	void	debug(void);
	void	info(void);
	void	warning(void);
	void	error(void);

	public:
	void	complain(std::string level);
};

