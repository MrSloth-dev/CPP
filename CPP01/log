CC = c++
NAME = harlFilter
SRCS = main.cpp Harl.cpp
CFLAGS = -Wall -Wextra -std=c++98 -g
OBJS = $(SRCS:.cpp=.o)


all : $(NAME)

$(NAME): $(OBJS)
	$(CC) $(CFLAGS) $(SRCS) -o $(NAME)

%.o: %.cpp
	@$(CC) $(CFLAGS) -c $< -o $@

clean :
	rm -fr *.o

fclean : clean
	rm -fr $(NAME)

re : fclean all

va : re
	valgrind -q --leak-check=full --show-leak-kinds=all ./$(NAME)

.PHONY : all fclean re
