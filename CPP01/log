#include "HumanB.hpp"

void HumanB::attack(){
	std::cout << this->_name << " attacks with their "
	<< this->_weapon->getType() << std::endl;
}

HumanB::HumanB(std::string name)
{
	this->_name = name;
}
void	HumanB::setWeapon(Weapon& weapon)
{
	this->_weapon = &weapon;
}
HumanB::~HumanB() {};
